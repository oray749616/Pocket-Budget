<?xml version="1.0" encoding="utf-8"?>
<data-extraction-rules>
    <!-- 
        数据提取规则配置 (Android 12+)
        定义在设备到设备传输期间哪些数据可以被提取
    -->
    
    <cloud-backup>
        <!-- 云备份规则 -->
        <include domain="database" />
        <include domain="sharedpref" />
        <exclude domain="cache" />
    </cloud-backup>
    
    <device-transfer>
        <!-- 设备传输规则 -->
        <include domain="database" />
        <include domain="sharedpref" />
        <exclude domain="cache" />
    </device-transfer>
</data-extraction-rules>
